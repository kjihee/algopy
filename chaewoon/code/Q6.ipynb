    "def hanoi(n,from_pos,to_pos,aux_pos):\n",
    "    if n == 1:\n",
    "        print(from_pos,\"->\",to_pos)\n",
    "        return\n",
    "    hanoi(n-1,from_pos,aux_pos,to_pos)\n",
    "    print(from_pos,\"->\",to_pos)\n",
    "    hanoi(n-1,aux_pos,to_pos,from_pos)\n",
